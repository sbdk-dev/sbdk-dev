# SBDK Development Environment Variables
# Copy this file to .env and customize for your environment

# Webhook Server Configuration
SBDK_WEBHOOK_HOST=0.0.0.0
SBDK_WEBHOOK_PORT=8000

# Database Configuration  
SBDK_DUCKDB_PATH=data/dev.duckdb

# Data Generation Configuration
SBDK_NUM_USERS=10000
SBDK_NUM_ORDERS=50000
SBDK_NUM_EVENTS=100000

# dbt Configuration
SBDK_DBT_PROFILES_DIR=~/.dbt
SBDK_DBT_TARGET=dev

# Pipeline Configuration
SBDK_PIPELINES_PATH=./pipelines
SBDK_DBT_PATH=./dbt